{
  "name": "browser-template",
  "version": "1.0.0",
  "license": "GPLV3",
  "repository": "https://github.com/unilasalle-amiens/modele-de-navigateur",
  "main": "main.js",
  "author": "UniLaSalle",
  "description": "Version d√©veloppement d'un squelette de navigateur ",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p serve start:electron",
    "build": "ng build",
    "serve": "ng serve",
    "start:electron": "wait-on tcp:4200 && electron . --serve",
    "build:mac": "ng build --base-href ./ && electron-builder -m --publish=never",
    "build:win": "ng build --base-href ./ && electron-builder -w --publish=never",
    "build:linux": "ng build --base-href ./ && electron-builder -l --publish=never",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.3",
    "@angular/cdk": "^18.2.3",
    "@angular/common": "^18.2.3",
    "@angular/compiler": "^18.2.3",
    "@angular/core": "^18.2.3",
    "@angular/forms": "^18.2.3",
    "@angular/material": "^18.2.3",
    "@angular/platform-browser": "^18.2.3",
    "@angular/platform-browser-dynamic": "^18.2.3",
    "@angular/router": "^18.2.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.3",
    "@angular/cli": "^18.2.3",
    "@angular/compiler-cli": "^18.2.3",
    "@types/jasmine": "~5.1.0",
    "electron": "^32.0.2",
    "electron-builder": "^25.0.5",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.5.4",
    "wait-on": "^8.0.0"
  },
  "build": {
    "directories": {
      "output": "releases"
    },
    "files": [
      "main.js",
      "preload.js",
      "dist/**/*"
    ]
  }
}
